{
      "name": "AnieBot",
      "description": "A Telethon based Telegram group management bot.",
      "logo": "https://telegra.ph/file/2991b4522d27c240a93bd.jpg",
      "keywords": [
         "Telethon",
         "Telegram",
         "Group",
         "manager",
         "Sammy"
      ],   
   "repository": "https://github.com/FriDayXD/AnieBot",
   "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "env": {
      "TOKEN": {
         "description": "Your bot token. Get one from @BotFather duh",
         "required": true,
         "value": "5019625580:AAHHTh_wyDh-LZmfM3NULXRGD0-vClVJ2nw"
      },
      "API_KEY": {
         "description": "Get API_KEY from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": "16043274"
      },
      "API_HASH": {
         "description": "Get API_HASH from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": "314d816924009b7eb70f0d372520b12b"
      },
      "OWNER_ID": {
         "description": "Your user ID as an integer.",
         "required": true,
         "value": "1668305941"
      },
      "OWNER_USERNAME": {
         "description": "Your username without the @",
         "value": "SammyXD"
      },
      "STRING_SESSION": {
         "description": "Telegram String Session",
         "required": false,
         "value": ""
      },
      "MONGO_DB_URI": {
         "description": "MongoDB Url get from mongodb",
         "required": false,
         "value": ""
      },      
      "BOT_ID": {
         "description": "Id of the bot",
         "required": true,
         "value": "1926801217"
      },
      "DEV_USERS": {
         "description": "Ids of dev users seperated by space",
         "required": false,
         "value": ""
      },
      "UPSTREAM_REPO_URL": {
         "description": "Required for Update module",
         "required": true,
         "value": "https://github.com/FriDayXD/AnieBot"
      },
      "HEROKU_API_KEY": {
         "description": "Heroku Account Api Key",
         "required": false,
         "value": ""
      },
      "HEROKU_APP_NAME": {
         "description": "Heroku APP Name",
         "required": false,
         "value": ""
      }
   },
   "keywords": [
      "telegram",
      "Telethon",
      "group",
      "manager",
      "Blaze"
   ],
   "name": "Sammy",
   "repository": "https://github.com/FriDayXD/AnieBot"
}
